@{
}
<h2>Welcome to Admin Login Page</h2>
<form method="post">
    <label>Enter Username</label>@Html.TextBoxFor(c=>c.username, new {@class="form-control"})<br /><br />
    <label>Enter password</label>@Html.PasswordFor(c=>c.password, new {@class="form-control"})<br /><br />
    <input type="submit" value="Login" class="btn btn-primary" />
</form>
<div>@ViewData["uid"]</div>
